{
  "Database": {
    "ConnectionString": "Server=localhost;Database=myapp;User=admin;Password=secret;",
    "Timeout": 30,
    "MaxPoolSize": 100
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Console": {
      "IncludeScopes": false
    },
    "MaskKeys": [ "password", "token" ]
  },
  "AppSettings": {
    "ApplicationName": "MyApplication",
    "Version": "1.0.0",
    "Environment": "Development",
    "Features": {
      "EnableCache": true,
      "EnableAnalytics": false,
      "MaxUploadSize": 10485760
    },
    "SystemUsers": [
      {
        "Id": "SampleUserId1",
        "Name": "SampleUser1"
      },
      {
        "Id": "SampleUserId2",
        "Name": "SampleUser2"
      }
    ],
    "Fields": {
      "Field1": {
        "Contents":{
          "Content1": "AAA",
          "Content2": "BBB",
          "Map": {
            "AAA": 1,
            "BBB": 2
          }
        }
      },
      "Field2": {
        "Contents":{
          "Content1": "AAA",
          "Content2": "BBB",
          "Map": {
            "CCC": 3,
            "DDD": 4
          }
        }
      }
     },
     "Keys": {
      "Key1": [
        {
          "Test": "AAA"
        }
      ],
      "Key2": [
        {
          "Test": "BBB"
        }
      ]
     }
  },
  "Email": {
    "SmtpServer": "smtp.example.com",
    "SmtpPort": 587,
    "UseSsl": true,
    "FromAddress": "noreply@example.com"
  }
}
